# ADK Configuration Template
# This is a template file committed to git
# Copy this file to .env and replace with your actual values

# Agent Engine Memory Service Resource ID
# Format: agentengine://YOUR_RESOURCE_ID
# Get your resource ID from Google Cloud Console > Agent Engine
# Example: MEMORY_SERVICE_URI=agentengine://1234567890
MEMORY_SERVICE_URI=agentengine://YOUR_RESOURCE_ID_HERE


GOOGLE_GENAI_USE_VERTEXAI=TRUE
GOOGLE_CLOUD_PROJECT=projectid
GOOGLE_CLOUD_LOCATION=us-central1